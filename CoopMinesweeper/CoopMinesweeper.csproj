<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <TypeScriptToolsVersion>Latest</TypeScriptToolsVersion>
    <Version>0.0.2</Version>
    <Authors>Stefan Timovski</Authors>
    <PackageProjectUrl>https://github.com/Timovski/Co-opMinesweeper</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Timovski/Co-opMinesweeper/blob/master/LICENSE.md</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/Timovski/Co-opMinesweeper</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>A 2-player cooperative browser-based variant of the game Minesweeper.</Description>
    <PackageTags>browser;cooperative;game</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Npgsql" Version="4.0.4" />
  </ItemGroup>

</Project>
